{
    "env": {
        "browser": true,
        "jquery": true,
        "greasemonkey": true
    },
    "extends": "eslint:recommended",
    "rules": {
        "indent": [
            "error",
            4
        ],
        "linebreak-style": [
            "error",
            "unix"
        ],
        "quotes": [
            "error",
            "single"
        ],
        "semi": [
            "error",
            "always"
        ],
        "no-console": 0,
        "globals": {
	        "saveAs": false,
	        "JSZip": false
	    }
    }
}
